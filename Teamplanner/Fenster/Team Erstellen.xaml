<Window x:Class="Teamplanner.Fenster.Team_Erstellen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Teamplanner.Fenster"
        mc:Ignorable="d"
        Title="Team_Erstellen" Height="191" Width="494" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="speichern" Content="Speichern" HorizontalAlignment="Left" Margin="166,108,0,0" Width="175" Height="57" VerticalAlignment="Top" Click="speichern_Click"/>
        <TextBox x:Name="Name" HorizontalAlignment="Left" Margin="44,55,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Owner" HorizontalAlignment="Left" Margin="267,55,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <GroupBox HorizontalAlignment="Left" Height="56" Header="Team Name" Margin="32,27,0,0" VerticalAlignment="Top" Width="217"/>
        <GroupBox HorizontalAlignment="Left" Height="56" Header="Owner" Margin="254,27,0,0" VerticalAlignment="Top" Width="217"/>

    </Grid>
</Window>
